<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <ProduceReferenceAssembly>true</ProduceReferenceAssembly>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>portable</DebugType>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\Fonts\B Yekan.ttf" />
    <None Remove="Resources\Images\ClosedEye.png" />
    <None Remove="Resources\Images\Email.png" />
    <None Remove="Resources\Images\Eye.png" />
    <None Remove="Resources\Images\IdCard.png" />
    <None Remove="Resources\Images\Lock.png" />
    <None Remove="Resources\Images\Person.png" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\Fonts\B Yekan.ttf" />
